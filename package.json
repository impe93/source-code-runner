  "devDependencies": {
    "@types/archiver": "^5.1.0",
    "@types/dockerode": "^3.2.1",
    "@types/jest": "^26.0.19",
    "@types/uuid": "^8.3.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
